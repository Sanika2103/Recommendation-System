   "source": [
    "Function to take only first three main cast of a movie"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "id": "42aec8b8",
   "metadata": {},
   "outputs": [],
   "source": [
    "def convertcast(text):\n",
    "    L = []\n",
    "    counter = 0\n",
    "    for i in ast.literal_eval(text):\n",
    "        if counter < 3:\n",
    "            L.append(i['name'])\n",
    "        counter+=1\n",
    "    return L \n"
   ]
  },
